{\n                var self = this\n                var url = 'http://123.57.45.31:9000/user/v1/user'\n                //使用post方法提交数据\n                axios\n                    .post(url, {\n                        userName: self.newUserName,\n                        userPassword: self.newUserPwd\n                    })\n                    .then(function (response) {\n                        //注册成功，跳转到登录页面\n                        if (response.data) self.$router.push({path: '/login'})\n                        else self.serverMsg = '注册失败，请重新尝试'\n                    })\n                    .catch(function (error) {\n                        //失败\n                        console.log(error)\n                        self.$router.push({path: '/error'})\n                    })\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components"}]}